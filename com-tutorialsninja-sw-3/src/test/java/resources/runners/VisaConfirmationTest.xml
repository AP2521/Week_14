<?xml version = "1.0" encoding = "UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="testsuite" verbose="1">
    <listeners>
        <listener class-name="sw_tutorialsninja_com.customlisteners.CustomListeners"></listener>
        <listener class-name="org.uncommons.reportng.HTMLReporter"/>
        <listener class-name="org.uncommons.reportng.JUnitXMLReporter"/>
    </listeners>
    <test name="TutorialNinja Test">
        <classes>
            <class name="sw_tutorialsninja_com.testsuite.DesktopTest"></class>
            <class name="sw_tutorialsninja_com.testsuite.LaptopsAndNotebooksTest"></class>
            <class name="sw_tutorialsninja_com.testsuite.MenuTest"></class>
        </classes>
    </test>
</suite>
